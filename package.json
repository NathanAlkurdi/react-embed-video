{
  "name": "react-embed-video",
  "version": "0.2.1",
  "description": "An easy to slot in component that renders a video embed of popular streaming services.",
  "author": "Nathan Al-Kurdi",
  "license": "MIT",
  "packageManager": "yarn@3.2.1",
  "main": "./dist/main.js",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NathanAlkurdi/react-embed-video.git"
  },
  "workspaces": [
    "preview"
  ],
  "scripts": {
    "build": "webpack --mode production && yarn typecheck",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "lint": "eslint \"{**/*,*}.{js,ts,jsx,tsx}\"",
    "lint:fix": "eslint \"{**/*,*}.{js,ts,jsx,tsx}\" --fix",
    "prepare": "yarn build",
    "prepublishOnly": "yarn test && yarn lint:fix && yarn lint",
    "start": "parcel src/preview/index.html",
    "test": "jest --config jest.config.js",
    "typecheck": "tsc --emitDeclarationOnly"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "react",
    "video",
    "embed",
    "embedding",
    "embedding video"
  ],
  "peerDependencies": {
    "react": ">=16"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.19.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.19.4",
    "@babel/plugin-proposal-optional-chaining": "^7.18.9",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@types/jest": "^28.1.4",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "babel-loader": "^9.0.0",
    "babel-plugin-jsx-remove-data-test-id": "^3.0.0",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "eslint": "^8.19.0",
    "eslint-plugin-jest-dom": "^4.0.2",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^28.1.2",
    "jest-canvas-mock": "^2.4.0",
    "jest-environment-jsdom": "^28.1.2",
    "mini-css-extract-plugin": "^2.6.1",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-jest": "^28.0.7",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  }
}
