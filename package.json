{
  "name": "react-embed-video",
  "version": "0.1.0",
  "description": "An easy to slot in component that renders a video embed from popular streaming services.",
  "author": "Nathan Al-Kurdi",
  "license": "MIT",
  "packageManager": "yarn@3.2.1",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NathanAlkurdi/react-embed-video.git"
  },
  "workspaces": [
    "preview"
  ],
  "scripts": {
    "build": "yarn build:esm && yarn build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "lint": "eslint \"{**/*,*}.{js,ts,jsx,tsx}\"",
    "lint:fix": "eslint \"{**/*,*}.{js,ts,jsx,tsx}\" --fix",
    "prepare": "yarn build",
    "prepublishOnly": "yarn test && yarn lint:fix && yarn lint",
    "start": "parcel src/preview/index.html",
    "test": "jest --config jest.config.js"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "react",
    "video",
    "embed",
    "embedding",
    "embedding video"
  ],
  "peerDependencies": {
    "react": ">=16"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@types/jest": "^28.1.4",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "eslint": "^8.19.0",
    "eslint-plugin-jest-dom": "^4.0.2",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^28.1.2",
    "jest-canvas-mock": "^2.4.0",
    "jest-environment-jsdom": "^28.1.2",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-jest": "^28.0.7",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  }
}
